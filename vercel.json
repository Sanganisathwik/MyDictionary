{
  "version": 2,
  "builds": [
    {
      "src": "backend/src/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/graphql",
      "dest": "backend/src/index.ts"
    },
    {
      "src": "/(.*)",
      "dest": "backend/src/index.ts"
    }
  ],
  "env": {
    "MONGODB_URI": "@mongodb_uri",
    "NODE_ENV": "production",
    "OXFORD_API_KEY": "@oxford_api_key",
    "OXFORD_API_URL": "@oxford_api_url"
  }
}
